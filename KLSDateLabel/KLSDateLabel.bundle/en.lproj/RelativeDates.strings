/* String for current time (relative date label) */
"now" = "now";

/* Pluralized string for number of second (relative date labels) */
"%d second (plural rule: one)" = "%d second";
"%d second (plural rule: other)" = "%d seconds";

/* Pluralized strings for number of minutes (relative date labels) */
"%d minute (plural rule: one)" = "%d minute";
"%d minute (plural rule: other)" = "%d minutes";

/* Pluralized strings for number of hours (relative date labels) */
"%d hour (plural rule: one)" = "%d hour";
"%d hour (plural rule: other)" = "%d hours";

/* Pluralized strings for number of days (relative date labels) */
"%d day (plural rule: one)" = "%d day";
"%d day (plural rule: other)" = "%d days";

/* Format string for past dates */
"ago" = "%@ ago";

/* Format string for future dates */
"from now" = "%@ from now";